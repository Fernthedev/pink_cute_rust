{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.4.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Pink Cute Rust",
    "id": "pink_cute_rust",
    "version": "0.1.0",
    "url": null,
    "additionalData": {
      "overrideSoName": "libpink_cute_rust.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "cargo ndk -t arm64-v8a -o ./build build --release"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "cpp": [
        "pwsh ./scripts/make_compile_db.ps1"
      ]
    },
    "qmodIncludeDirs": [
      "build/arm64-v8a"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "pink_cute_rust.qmod"
  },
  "dependencies": [
    {
      "id": "bs-cordl",
      "versionRange": "^4007.0.0",
      "additionalData": {
        "includeQmod": false,
        "private": true
      }
    }
  ]
}